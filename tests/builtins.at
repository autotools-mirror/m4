translit(`abcdeabcde', `a', `fg')
translit(`abcdeabcde', `ab', `f')
translit(`abcdeabcde', `ab', `fg')
translit(`abcdeabcde', `ab', `ba')
translit(`abcdeabcde', `e', `f')
translit(`abc', `', `cde')
translit(`', `a', `bc')
]])
